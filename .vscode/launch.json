    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "JAVA: Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "py4j test",
            "type": "python",
            "request": "launch",
            "program": "./practice/py4j_test.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ],
    "compounds": [
        {
            "name": "start Python & JAVA",
            "configurations": ["Launch StackEntryPoint", "py4j test"]
        }
    ]

}